  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andrelas1/post-date/issues"
  },
  "homepage": "https://github.com/andrelas1/post-date#readme",
  "devDependencies": {
    "nodemon": "^2.0.15",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "mocha": "^9.1.3",
    "ts-node": "^10.4.0"
  }
}
